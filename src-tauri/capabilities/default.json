{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "store:default",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://vrchat.cloud/*"
        },
        {
          "url": "https://vrchat.com/*"
        },
        {
          "url": "https://api.vrchat.cloud/*"
        }
      ]
    },
    "websocket:default",
    "notification:default"
  ]
}